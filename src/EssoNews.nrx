class EssoNews

  properties static
  zipFile = 'BNA-DIG-ESSONEWS-1984_images.zip'
  /**
   * Default constructor
   */
  method EssoNews()
    return

  method main(args=String[]) static
    say Date() "EssoNews Started"
    issueMap = TreeMap()
    say Date() "EssoNews Reading" zipFile
    a	     = Zip().readZip(zipFile)
    i	     = a.iterator()
    loop while i.hasNext()
      filename_ = i.next()
      parse filename_ 'ESSONEWS-' issue '-' .
      if issueMap.get(issue) == null then
	do
	  b = ArrayList()
	  b.add(filename_)
	end
      else
	do
	  b = ArrayList issueMap.get(issue)
	  b.add(fileName_)
	end
      issueMap.put(issue,b)
    end
    j = issueMap.keySet().iterator()
    loop while j.hasNext()
      key= j.next()
      say Date() "EssoNews for key" key
      toUnzip = ArrayList issueMap.get(key)
      zi = Zip()
      say Date() "EssoNews Unzipping" toUnzip
      zi.extractFilesFromZip(zipFile,toUnzip)
      say Date() 'EssoNews Zipping into' zipFile.left(21)||'-'||key'_images.zip'
      zo=Zip(zipFile.left(21)||'-'||key'_images.zip')
      zo.setFileNames(toUnzip)
      zo.create()
      zo.deleteOriginals()
    end
    say Date() "EssoNews Ended"